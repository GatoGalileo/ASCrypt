package org.ascrypt.common 
{
	/**
	* Constant values for available operation modes.
	* @author Mika Palmu
	*/
	public class OperationMode
	{
		/**
		* Operation mode id for ECB.
		*/
		public static const ECB:String = "ecb";
		
		/**
		* Operation mode id for CBC.
		*/
		public static const CBC:String = "cbc";
		
		/**
		* Operation mode id for CTR.
		*/
		public static const CTR:String = "ctr";
		
		/**
		* Operation mode id for NONE.
		*/
		public static const NONE:String = "none";
		
	}
	
}
